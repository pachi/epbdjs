{
  "name": "epbdpanel",
  "version": "0.2.0",
  "description": "Web panel for the epbdcalc library",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "stats": "webpack --profile --json > stats.json",
    "statsprod": "NODE_ENV=production webpack -p --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json build",
    "clean": "rm -rf build/",
    "buildprod": "NODE_ENV=production webpack --progress --colors -p",
    "builddev": "NODE_ENV=development webpack --progress -d --colors --watch",
    "webpack-server": "webpack-dev-server --hot --inline --colors",
    "web-server": "http-server -p 3000 ./build",
    "prodserver": "npm run web-server | npm run buildprod",
    "bs3": "rm .bootstraprc && cp node_modules/bootstrap-loader/.bootstraprc-3-default .bootstraprc",
    "lint": "eslint --ignore-pattern build/*.js --ext .js,.jsx app/",
    "start": "npm run webpack-server",
    "deploy": "NODE_ENV=production webpack -p"
  },
  "keywords": [
    "energy",
    "buildings",
    "CTE",
    "energ√≠a",
    "edificios"
  ],
  "author": "Rafael Villar Burke",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "img-loader": "^2.0.0",
    "imports-loader": "^0.7.1",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^3.0.0-beta.7",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "d3": "^3.5.17",
    "dimple": "git+https://github.com/PMSI-AlignAlytics/dimple.git#2.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.0.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.4.0",
    "redux-thunk": "^2.1.0"
  }
}
