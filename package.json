{
  "name": "epbdpanel",
  "version": "0.1.0",
  "description": "Web panel for the pyepbd (epbdcalc) library",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build/",
    "buildprod": "NODE_ENV=production webpack --progress --colors -p",
    "builddev": "NODE_ENV=development webpack --progress -d --colors --watch",
    "webpack-server": "webpack-dev-server --hot --inline --colors",
    "web-server": "http-server -p 3000 ./build",
    "prodserver": "npm run web-server | npm run buildprod",
    "bs3": "rm .bootstraprc && cp node_modules/bootstrap-loader/.bootstraprc-3-default .bootstraprc",
    "lint": "eslint --ignore-pattern build/*.js --ext .js,.jsx app/",
    "start": "npm run webpack-server",
    "deploy": "NODE_ENV=production webpack -p"
  },
  "keywords": [
    "energy",
    "buildings",
    "CTE",
    "energ√≠a",
    "edificios"
  ],
  "author": "Rafael Villar Burke",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.9",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.16.0",
    "http-server": "^0.9.0",
    "img-loader": "^1.2.2",
    "imports-loader": "^0.6.5",
    "lodash-webpack-plugin": "^0.10.1",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.10.0",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "d3": "^3.5.17",
    "dimple": "git+https://github.com/PMSI-AlignAlytics/dimple.git#2.2.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "redux": "^3.4.0",
    "redux-thunk": "^2.1.0"
  }
}
